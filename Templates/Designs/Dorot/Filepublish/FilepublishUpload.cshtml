<form action="/admin/public/Upload.aspx" method="post" enctype="multipart/form-data">
	<input type="hidden" name="Notify" value="@GetValue("FilepublishUploadNotify")">
	<input type="hidden" name="Folder" value="@GetValue("FilepublishUploadFolder")">
	<input type="hidden" name="Subject" value="@GetValue("FilepublishUploadSubject")">
	<input type="hidden" name="Recipient" value="@GetValue("FilepublishUploadRecipient")">
	<input type="hidden" name="RecipientCC" value="@GetValue("FilepublishUploadRecipientCC")">
	<input type="hidden" name="RecipientBCC" value="@GetValue("FilepublishUploadRecipientBCC")">
	<input type="hidden" name="Text" value="@GetValue("FilepublishUploadText")">
	<input type="hidden" name="Confirm" value="@GetValue("FilepublishUploadConfirm")">
	<table>
		<tr>
			<td><b>&nbsp;Upload</b></td>
			<td><input type="File" name="File" style="width:250px;"></td>
		</tr>
		<tr>
			<td align="right" colspan="2"><input type="submit" value="Upload"></td>
		</tr>
	</table>
</form>